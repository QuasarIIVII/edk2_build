    SECTION .text

;------------------------------------------------------------------------------
; UINTN
; __attribute__((cdecl))
; AsmMwait (
; UINTN Eax,
; UINTN Ecx
; );
;------------------------------------------------------------------------------
global AsmMwait
AsmMwait:
    mov eax, [esp + 4]
    mov ecx, [esp + 8]
    mwait
    ret
